declare field[x,y] :: Int ∈ {1..9} for x ∈ {1..9}, y ∈ {1..9}

% every number occurs once in each row
∀ i ∈ {1..9}.∀ y ∈ {1..9}.∃ x ∈ {1..9}. field[x,y] ≥ i ∧ field[x,y] ≤ i
% every number occurs once in each column
∀ i ∈ {1..9}.∀ x ∈ {1..9}.∃ y ∈ {1..9}. field[x,y] ≥ i ∧ field[x,y] ≤ i
% every number occurs once in each block
∀ i ∈ {1..9}.∀ x1 ∈ {0..2}.∀ y1 ∈ {0..2}.∃x2 ∈ {1..3}.∃y2 ∈ {1..3}.field[3*x1+x2,3*y1+y2] ≥ i ∧ field[3*x1+x2,3*y1+y2] ≤ i

field[2,1] ≥ 4 ∧ field[2,1] ≤ 4
field[2,2] ≥ 9 ∧ field[2,2] ≤ 9
field[1,3] ≥ 5 ∧ field[1,3] ≤ 5

===============

for y := 1 to 9 do {
  for x := 1 to 9 do {
    print(field[x,y], ' ')
    if x = 3 ∨ x = 6 then print('| ')
  }
  println()
  if y = 3 ∨ y = 6 then println('------+-------+------')
}

