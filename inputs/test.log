define N 3
define SIZE N*N

declare field[x,y] :: Int ∈ {1..SIZE} for x ∈ {1..SIZE}, y ∈ {1..SIZE}

# every number occurs once in each row
∀ i ∈ {1..SIZE}.∀ y ∈ {1..SIZE}.∃ x ∈ {1..SIZE}. field[x,y] = i
# every number occurs once in each column
∀ i ∈ {1..SIZE}.∀ x ∈ {1..SIZE}.∃ y ∈ {1..SIZE}. field[x,y] = i
# every number occurs once in each block
∀ i ∈ {1..SIZE}.∀ x1 ∈ {0..N-1}.∀ y1 ∈ {0..N-1}.∃x2 ∈ {1..N}.∃y2 ∈ {1..N}.field[N*x1+x2,N*y1+y2] = i

