declare queen[x,y] :: Bool for x ∈ {1..8}, y ∈ {1..8}

% there is a queen in every row
∀ y ∈ {1..8}.∃ x ∈ {1..8}.queen[x,y]
% there is only one queen per row
∀ y ∈ {1..8}.∀ x1 ∈ {1..8-1}.∀ x2 ∈ {x1+1..8}.queen[x1,y] → ¬queen[x2,y]
% there is only one queen per column
∀ x ∈ {1..8}.∀ y1 ∈ {1..8-1}.∀ y2 ∈ {y1+1..8}.queen[x,y1] → ¬queen[x,y2]
% there is only one queen per diagonal
∀ x1 ∈ {1..8-1}.∀ y1 ∈ {1..8}.∀ x2 ∈ {x1+1..8}.∀ y2 ∈ {1..8} with x1-y1 = x2-y2.¬queen[x1,y1] ∨ ¬queen[x2,y2]
∀ x1 ∈ {1..8-1}.∀ y1 ∈ {1..8}.∀ x2 ∈ {x1+1..8}.∀ y2 ∈ {1..8} with x1+y1 = x2+y2.¬queen[x1,y1] ∨ ¬queen[x2,y2]

===============

for y := 1 to 8 do {
  for x := 1 to 8 do {
    if queen[x,y] then print("Q")
    else print(".")
  }
  println()
}

