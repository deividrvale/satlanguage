define SIZE 50

declare queen[x,y] :: Bool for x ∈ {1..SIZE}, y ∈ {1..SIZE}

# there is a queen in every row
∀ y ∈ {1..SIZE}.∃ x ∈ {1..SIZE}.queen[x,y]
# there is only one queen per row
∀ y ∈ {1..SIZE}.∀ x1 ∈ {1..SIZE-1}.∀ x2 ∈ {x1+1..SIZE}.queen[x1,y] → ¬queen[x2,y]
# there is only one queen per column
∀ x ∈ {1..SIZE}.∀ y1 ∈ {1..SIZE-1}.∀ y2 ∈ {y1+1..SIZE}.queen[x,y1] → ¬queen[x,y2]
# there is only one queen per diagonal
∀ x1 ∈ {1..SIZE-1}.∀ y1 ∈ {1..SIZE}.∀ x2 ∈ {x1+1..SIZE}.∀ y2 ∈ {1..SIZE} with x1-y1 = x2-y2.¬queen[x1,y1] ∨ ¬queen[x2,y2]
∀ x1 ∈ {1..SIZE-1}.∀ y1 ∈ {1..SIZE}.∀ x2 ∈ {x1+1..SIZE}.∀ y2 ∈ {1..SIZE} with x1+y1 = x2+y2.¬queen[x1,y1] ∨ ¬queen[x2,y2]

===============

for y := 1 to SIZE do {
  for x := 1 to SIZE do {
    if queen[x,y] then print("Q")
    else print(".")
  }
  println()
}

